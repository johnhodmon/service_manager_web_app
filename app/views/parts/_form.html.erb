<%= form_for(@part) do |f| %>


  <div class="field">
    <%= f.label :part_number %><br>
    <%= f.text_field :part_number %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>
  <div class="form-group">
    <%= f.label :cost %><br>
    <%= f.text_field :cost %>
  </div>
  <div class="form-group">
    <%= f.label :quantity_in_stock %><br>
    <%= f.number_field :quantity_in_stock %>
  </div>
  
    <%= f.submit "Create Part" ,:class=>"btn btn-primary"%>
  
<% end %>
